generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Inventory {
  id      Int    @id @default(autoincrement())
  name    String
  status  Status
  remarks String @default("")

  @@map("inventories")
}

enum Status {
  IN_STOCK
  OUT_OF_STOCK
  IN_USE
}
